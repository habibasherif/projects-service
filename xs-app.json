{
  "welcomeFile": "HTML5Module/index.html",
  "authenticationMethod": "none",
  "websockets": {
    "enabled": true
  },
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/NodeWS(.*)$",
      "target": "$1",
      "authenticationType": "none",
      "destination": "NodeWs_api",
      "csrfProtection": false
    },
    {
      "source": "^/odataSrv/(.*)$",
      "target": "/catalog/$1",
      "authenticationType": "none",
      "destination": "srv_api",
      "csrfProtection": false
    },
    {
      "source": "^/HTML5Module/(.*)$",
      "target": "$1",
      "authenticationType": "none",
      "localDir": "../HTML5Module/webapp"
    }
  ]
}